{
  "manifest_version": 2,
  "name": "SiSU Helper",
  "version": "1.0",
  "description": "SiSU helper",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://sisu.mec.gov.br/*"
      ],
      "js": [
        "sisu.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "SiSU Helper"
  }
}